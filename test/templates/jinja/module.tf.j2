module "{{ terraform_module_base__hostname }}" {

  # Base
  source                      = "{{ terraform_module_base__source }}?ref={{ terraform_module_base__module_version }}"
  hostname                    = "{{ terraform_module_base__hostname }}"
  datacenter                  = "{{ terraform_module_base__datacenter }}"
  datastore_cluster           = "odn1-data-s2"
  disks                       = {{ terraform_module_base__disks }}
  folder                      = "{{ terraform_module_base__folder }}"
  memory                      = {{ terraform_module_base__memory }}
  vcpus                       = {{ terraform_module_base__vcpus }}
  resource_pool               = "{{ terraform_module_base__resource_pool }}"
  template_vm                 = "{{ terraform_module_base__template_vm }}"
  vlan                        = ["vlan-601"]
  ignition_encoding           = "{{ terraform_module_base__ignition_encoding }}"
  adapter_type                = ["vmxnet3"]
  wait_for_guest_net_routable = false
  wait_for_guest_net_timeout  = 0
  thin_provisioned            = false
  eagerly_scrub               = true
  skip_customization          = true
  ip_address                  = ["10.50.61.198"]
  ip_address_prefix           = ["24"]
  ip_address_gateway          = ["10.50.61.1"]
  ip_address_familiy          = "ipv4"
  network_interface_mtu       = 1500
  dns_servers                 = ["1.1.1.1", "8.8.8.8", "8.8.4.4"]
  ssh_authorized_keys         = ["ssh-rsa  cardno:000606708586"]
  sshd_port                   = 34

  providers = {
    vsphere = vsphere.{{ terraform_module_base__provider }}
  }

}
